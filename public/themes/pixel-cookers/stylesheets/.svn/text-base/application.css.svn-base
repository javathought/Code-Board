div.projects h3 {
	background: none;
	padding-left: 0;
}
div.projects > h3 {
	background: url(../images/22x22/projects.png) no-repeat scroll 0 50% transparent;
	padding-left: 26px;
}
div.news h3 {
	background-image: url(../images/22x22/notifications.png);
